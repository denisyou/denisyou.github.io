<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.rick.tablayoutdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Prototype2"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".Register" />
        <activity android:name=".Tabtest" />
        <activity android:name=".StudentStatus" />
        <activity
            android:name=".fortestpage"
            android:label="@string/title_activity_fortestpage"
            android:theme="@style/AppTheme" />
        <activity android:name=".CoursePost"></activity>
    </application>

</manifest>